{
	"$schema": "https://json-schema.org/schema",
	"$id": "Entity",
	"title": "",
	"type": "object",
	"properties": {
		"name": {
			"type": "string",
			"description": "Entity name to use (camelCase)",
			"$default": {
				"$source": "argv",
				"index": 0
			},
			"x-prompt": "Entity name to use (camelCase)"
		},
		"namePlural": {
			"type": "string",
			"required": "false",
			"description": "Plural name to use (camelCase)> Defaults to `${name}s`"
		}
	},
	"required": [
		"name"
	]
}
