import { IEntity } from "../entity/entity.type";

export interface <%= clazz %> extends IEntity {
<% for (let field of fields) { %>
	<%= field.name %>: <%= field.type %>;
<%  } %>
}

export interface <%= clazz %>State {
	<%= name %>s: Record<string, <%= clazz %>>;
}

export const select<%= clazz %>s = (state: <%= clazz %>State) => state.<%= name %>s;
