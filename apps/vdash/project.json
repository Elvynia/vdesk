{
	"name": "vdash",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "application",
	"prefix": "lv",
	"sourceRoot": "apps/vdash/src",
	"tags": [],
	"targets": {
		"build": {
			"executor": "@angular-devkit/build-angular:application",
			"outputs": [
				"{options.outputPath}"
			],
			"options": {
				"outputPath": "dist/apps/vdash",
				"index": "apps/vdash/src/index.html",
				"browser": "apps/vdash/src/main.ts",
				"polyfills": [
					"zone.js"
				],
				"tsConfig": "apps/vdash/tsconfig.app.json",
				"inlineStyleLanguage": "scss",
				"assets": [
					{
						"glob": "**/*",
						"input": "apps/vdash/public"
					}
				],
				"styles": [
					"apps/vdash/src/styles.scss",
					"libs/styles/material.scss"
				],
				"scripts": []
			},
			"configurations": {
				"production": {
					"budgets": [
						{
							"type": "initial",
							"maximumWarning": "500kb",
							"maximumError": "1mb"
						},
						{
							"type": "anyComponentStyle",
							"maximumWarning": "4kb",
							"maximumError": "8kb"
						}
					],
					"outputHashing": "all",
					"fileReplacements": [
						{
							"replace": "apps/vdash/src/environments/environment.ts",
							"with": "apps/vdash/src/environments/environment.prod.ts"
						}
					]
				},
				"development": {
					"optimization": false,
					"extractLicenses": false,
					"sourceMap": true
				}
			},
			"defaultConfiguration": "production"
		},
		"serve": {
			"executor": "@angular-devkit/build-angular:dev-server",
			"configurations": {
				"production": {
					"buildTarget": "vdash:build:production"
				},
				"development": {
					"buildTarget": "vdash:build:development",
					"port": 6401
				}
			},
			"defaultConfiguration": "development",
			"continuous": true
		},
		"extract-i18n": {
			"executor": "@angular-devkit/build-angular:extract-i18n",
			"options": {
				"buildTarget": "vdash:build"
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint"
		},
		"serve-static": {
			"executor": "@nx/web:file-server",
			"options": {
				"buildTarget": "vdash:build",
				"staticFilePath": "dist/apps/vdash/browser",
				"spa": true
			}
		}
	}
}
